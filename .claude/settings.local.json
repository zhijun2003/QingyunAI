{
  "permissions": {
    "allow": [
      "Bash(dir:*)",
      "Bash(tree:*)",
      "Bash(cat:*)",
      "Bash(npx nuxi@latest init . --force --packageManager pnpm --gitInit false)",
      "Bash(mkdir:*)",
      "Bash(where pnpm npm node)",
      "Bash(npm install:*)",
      "Bash(pnpm install:*)",
      "Bash(set CI=true)",
      "Bash(if exist node_modules rmdir /s /q node_modules)",
      "Bash(if exist pnpm-lock.yaml del pnpm-lock.yaml)",
      "Bash(git init:*)",
      "Bash(pnpm exec husky:*)",
      "Bash(for dir in packages/database packages/ai-runtime packages/billing packages/constants packages/types)",
      "Bash(do mkdir -p \"$dir/src\")",
      "Bash(echo:*)",
      "Bash(\"$dir/src/index.ts\")",
      "Bash(done)",
      "Bash(git add:*)",
      "Bash(git config:*)",
      "Bash(git commit:*)",
      "Bash(npx prisma generate:*)",
      "Bash(git show:*)",
      "Bash(FILTER_BRANCH_SQUELCH_WARNING=1 git filter-branch:*)",
      "Bash(git checkout:*)"
    ],
    "deny": [],
    "ask": []
  }
}
